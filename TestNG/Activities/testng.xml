<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FSTTests" thread-count="4" parallel="classes">
    <!-- Comment out groups to run everything -->
	<groups>
        <run>
            <include name="HeaderTests" />
        </run>
    </groups>
    <parameter name="username" value="admin" />
    <parameter name="password" value="password" />
    <test name="FSTActivities">
        <classes>
            <class name="activity.Activity_1" />
            <class name="activity.Activity_2" />
            <class name="activity.Activity_3" />
            <class name="activity.DemoOne" />
            <class name="activity.DemoTwo" />
            <class name="activity.Activity_5" />
            <class name="activity.Activity_6" />
            <class name="activity.Activity_7" />
        </classes>
    </test>
</suite>